<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    
    <servlet>
        <servlet-name>Admin_Login</servlet-name>
        <servlet-class>Controller.Admin_Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_insert</servlet-name>
        <servlet-class>Controller.Servlet_insert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_table</servlet-name>
        <servlet-class>Controller.Servlet_table</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin_logout</servlet-name>
        <servlet-class>Controller.Admin_logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Admin_Login</servlet-name>
        <url-pattern>/Admin_Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_insert</servlet-name>
        <url-pattern>/Servlet_insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_table</servlet-name>
        <url-pattern>/Servlet_table</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin_logout</servlet-name>
        <url-pattern>/Admin_logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
   </web-app>
